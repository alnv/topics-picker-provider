    "preferred-install": "dist"
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/topics-picker-provider"
      }
    },
    "contao-manager-plugin": "Alnv\\TopicsPickerProviderBundle\\ContaoManager\\Plugin"
  }
}