{
  "name": "alnv/topics-picker-provider",
  "type": "contao-bundle",
  "license": "proprietary",
  "authors": [
    {
      "name": "Alexander Naumov",
      "homepage": "https://alexandernaumov.de",
      "role": "Developer"
    }
